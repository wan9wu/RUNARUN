    <style name="CustomSeekbarStyle">
        <item name="android:maxHeight">2dp</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@color/colorAccent</item>
        <item name="android:progressDrawable">@drawable/gas_seekbar_progress</item>
        <item name="android:minHeight">2dp</item>
        <!--<item name="android:thumb">@mipmap/thumb</item>-->
    </style>

    <style name="StyleProgressBarMini" parent="Widget.AppCompat.ProgressBar.Horizontal">

        <item name="android:maxHeight">2dp</item>
        <item name="android:minHeight">2dp</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">
            @color/gray_cfcfcf
        </item>
    </style>

