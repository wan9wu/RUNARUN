<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 白色背景，无标题栏 -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@drawable/theme_splash</item>
    </style>

    <style name="AppTheme_yu" parent="@android:style/Theme.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:animationDuration">0</item>
        <item name="android:windowDisablePreview">true</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <!-- ****对话框部分 begin**** -->
    <!-- @android:style/Theme.Dialog -->
    <style name="BaseDialog">
        <item name="android:windowBackground">@drawable/transparent_background</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="ImageCropperDialogAnim" parent="@android:style/Animation.Dialog"></style>
    <!-- @android:style/Theme.Dialog -->
    <style name="checkinDialog">
        <item name="android:windowBackground">@drawable/transparent_background</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>


    <!-- 自定义loading dialog -->
    <style name="loading_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/black_90</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>


    <!-- 操作列表字体 -->
    <style name="ActionSheetTitleText">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#888888</item>
        <item name="android:textStyle">normal</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="ActionSheetText">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#037dc3</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>

    <!-- Navigation Header -->
    <style name="NavPage">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
        <!-- <item name="android:background">@drawable/nav_page</item> -->
        <item name="android:background">@color/white</item>
    </style>


    <!-- 操作列表字体 -->
    <style name="SelectorTitleText">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#b2b2b2</item>
        <item name="android:textStyle">normal</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="SelectorText">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>

    <!-- LoadingView对话框 -->
    <style name="LoadingDialog" parent="BaseDialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/LoadingDialogAnim</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>

    <style name="LoadingDialogAnim" parent="@android:style/Animation.Dialog"></style>
    <!-- 对话框字体 -->
    <style name="DialogLoadingText">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textStyle">normal</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>

    <!-- Message对话框 -->
    <style name="MessageDialog" parent="BaseDialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/MessageDialogAnim</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>

    <style name="MessageDialogAnim" parent="@android:style/Animation.Dialog"></style>
    <!-- 对话框字体 -->
    <style name="DialogMessageText">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textStyle">normal</item>
        <item name="android:singleLine">false</item>
        <item name="android:maxLines">3</item>
        <item name="android:gravity">center</item>
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:ellipsize">end</item>
    </style>

    <!-- 版本更新对话框 -->
    <style name="UpgradeDialog" parent="BaseDialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/UpgradeDialogAnim</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>

    <style name="UpgradeDialogAnim" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>
